	at java.base/jdk.internal.reflect.Reflection.newIllegalAccessException(Reflection.java:401)&lt;br/&gt;
	at java.base/java.lang.reflect.AccessibleObject.checkAccess(AccessibleObject.java:692)&lt;br/&gt;
	at java.base/java.lang.reflect.Method.invoke(Method.java:563)&lt;br/&gt;
	at com.sun.javatest.regtest.agent.MainWrapper$MainTask.run(MainWrapper.java:138)&lt;br/&gt;
	at java.base/java.lang.Thread.run(Thread.java:1491)&lt;br/&gt;
